{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreDependencies": [
    "@tailwindcss/typography",
    "mdast-util-to-string",
    "tw-animate-css",
    "tailwindcss"
  ],
  "entry": [
    "src/pages/**/*.{astro,ts}",
    "src/content/**/*.ts",
    "scripts/**/*.{js,ts}"
  ],
  "project": ["src/**/*.{ts,tsx,astro}"],
  "ignore": ["**/*.test.{ts,tsx}", "dist/**", ".astro/**", "node_modules/**"],
  "astro": {
    "entry": ["astro.config.mjs"]
  }
}
